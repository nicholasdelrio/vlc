<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	
	<display-name>Archetype Created Web Application</display-name>

 	 <!--  CORS Filter -->
  	<filter>
		<filter-name>CORS</filter-name>
		<filter-class>com.thetransactioncompany.cors.CORSFilter</filter-class>
 	</filter>

 	<filter-mapping>
    	<filter-name>CORS</filter-name>
    	<url-pattern>/*</url-pattern>
 	</filter-mapping>

	<servlet>
    	<description>Generate Projects-to-Projects via Common People Graph</description>
    	<display-name>Projects2Projects_People</display-name>
    	<servlet-name>Projects2Projects_People</servlet-name>
    	<servlet-class>edu.utep.cybershare.vlc.visualization.transformers.servlets.Servlet_Projects2Projects_People</servlet-class>
  	</servlet>

	<servlet>
    	<description>Generate Projects-to-People via PIShip Graph</description>
    	<display-name>Projects2People_PIShip</display-name>
    	<servlet-name>Projects2People_PIShip</servlet-name>
    	<servlet-class>edu.utep.cybershare.vlc.visualization.transformers.servlets.Servlet_Projects2People_PIShip</servlet-class>
  	</servlet>

  	<servlet-mapping>
    	<servlet-name>Projects2People_PIShip</servlet-name>
    	<url-pattern>/Projects2People_PIShip</url-pattern>
  	</servlet-mapping> 
  
  	<servlet-mapping>
    	<servlet-name>Projects2Projects_People</servlet-name>
    	<url-pattern>/Projects2Projects_People</url-pattern>
  	</servlet-mapping> 
</web-app>